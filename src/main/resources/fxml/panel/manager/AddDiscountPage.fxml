    <AnchorPane prefHeight="400.0" prefWidth="600.0">
        <TextField fx:id="percent" layoutX="76.0" layoutY="42.0" prefHeight="55.0" promptText="Percent" styleClass="textfield" AnchorPane.leftAnchor="76.0" />
        <TextField fx:id="usableTime" layoutX="76.0" layoutY="106.0" prefHeight="55.0" promptText="Usable Time" styleClass="textfield" AnchorPane.leftAnchor="76.0" />
        <TextField fx:id="startAt" layoutX="76.0" layoutY="171.0" prefHeight="55.0" promptText="Start At" styleClass="textfield" AnchorPane.leftAnchor="76.0" />
        <TextField fx:id="finishAt" layoutX="76.0" layoutY="236.0" prefHeight="55.0" promptText="Finish At" styleClass="textfield" AnchorPane.leftAnchor="76.0" />
