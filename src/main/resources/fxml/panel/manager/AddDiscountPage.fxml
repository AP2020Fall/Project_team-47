        <TextField fx:id="userNumber" layoutX="76.0" layoutY="367.0" prefHeight="55.0" promptText="User Number" styleClass="textfield" />
        <Button layoutX="77.0" layoutY="454.0" mnemonicParsing="false" onAction="#giveGift" prefHeight="50.0" prefWidth="145.0" text="Give Gift">
            <font>
                <Font name="Sakkal Majalla" size="25.0" />
            </font>
