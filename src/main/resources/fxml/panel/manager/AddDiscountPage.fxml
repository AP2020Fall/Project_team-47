            </font>
        </Button>
        <Button layoutX="604.0" layoutY="116.0" mnemonicParsing="false" onAction="#removeUser" prefHeight="55.0" prefWidth="150.0" text="Remove">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button layoutX="717.0" layoutY="547.0" mnemonicParsing="false" onAction="#addCode" prefHeight="55.0" prefWidth="145.0" text="Add Code">
            <font>
                <Font name="Sakkal Majalla" size="25.0" />
            </font>
        </Button>
        <Button layoutX="717.0" layoutY="628.0" mnemonicParsing="false" onAction="#back" prefHeight="55.0" prefWidth="145.0" text="Back">
